<p id="notice"><%= notice %></p>

<h1>Prices</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Name</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.price %></td>
        <td><%= price.name %></td>
        <td><%= price.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

