
<p id="notice"><%= notice %></p>

<h1>Prices for <%= params[:id]%> </h1>

<%= link_to "Back to all prices", new_price_path   %>

<table>
  <thead>
  <tr>
    <th>Price</th>
    <th>Name</th>
    <th>Time</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @prices.each do |price| %>
    <tr>
      <td><%= price.price %></td>
      <td><%= price.name %></td>
      <td><%= price.created_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

